/* components/projectExp/index.wxss */
.project-list {
   font-size: 24rpx;
   margin-top: 20rpx;
}
.project-list:first-child{
   margin-top: 0;
}
.icon-computer {
   width: 32rpx;
   height: 32rpx;
   vertical-align: -6rpx;
   margin-right: 10rpx;
}
.project-name .computer{
   color: #333;
   margin-right: 30rpx;
}
.project-desc{
   display: flex;
   flex-direction: row;
   justify-content: start;
   padding-left:42rpx;
   font-size: 22rpx;
   color: #666;
}
.project-desc .desc{
   flex: 1;
   text-align: justify;
   word-wrap: break-word;
   word-break: break-all;
}